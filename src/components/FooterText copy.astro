---
export interface Props {
  body?: string;
  icon?: string;
}

const {
  body = '10 Ogundare Awise Street, by Deeper Life estate, Gbagada Soluyi, Lagos State, Nigeria.',
  icon = 'location_on',
} = Astro.props;
---

<div class="info">
  <span class="material-icons">{icon}</span>
  <p>{body}</p>
</div>

<style>
  
  h2 {
    margin-left: 25px;
  }
</style>
